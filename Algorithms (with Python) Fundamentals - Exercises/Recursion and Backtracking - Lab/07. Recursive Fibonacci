def fibonacci(num):
    if num <= 1:
        return 1

    return fibonacci(num - 1) + fibonacci(num - 2)


number = int(input())
print(fibonacci(number))





def iterative_fib(number):
    fib0 = 1
    fib1 = 1
    result = 0
    for _ in range(number - 1):
        result = fib0 + fib1
        fib0, fib1 = fib1, result
    return result


num = int(input())
print(iterative_fib(num))
