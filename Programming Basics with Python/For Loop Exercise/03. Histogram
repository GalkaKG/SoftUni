n = int(input())
p1 = 0
p2 = 0
p3 = 0
p4 = 0
p5 = 0

for _ in range(n):
    num = int(input())

    if num < 200:
        p1 += 1
    elif num <= 399:
        p2 += 1
    elif num <= 599:
        p3 += 1
    elif num <= 799:
        p4 += 1
    elif num >= 800:
        p5 += 1

p1_prc = p1 / n * 100
p2_prc = p2 / n * 100
p3_prc = p3 / n * 100
p4_prc = p4 / n * 100
p5_prc = p5 / n * 100

print(f'{p1_prc:.2f}%')
print(f'{p2_prc:.2f}%')
print(f'{p3_prc:.2f}%')
print(f'{p4_prc:.2f}%')
print(f'{p5_prc:.2f}%')
