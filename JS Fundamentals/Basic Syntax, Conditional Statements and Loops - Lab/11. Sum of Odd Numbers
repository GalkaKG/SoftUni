function sumOfOddNumbers (n) {
    let counter = 0;
    let sum = 0;

    for (let i = 1; i <= 100; i++) {
        if (counter >= n) {
            break;
        }

        if (i % 2 !== 0) {
            counter++
            console.log(i);
            sum += i
        }
    }
    console.log(`Sum: ${sum}`);
}
