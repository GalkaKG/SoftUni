number = int(input())
all_numbers_are_even = False

for _ in range(number):
    num = int(input())
    all_numbers_are_even = False

    if num % 2 != 0:
        print(f'{num} is odd!')
        break
    else:
        all_numbers_are_even = True

if all_numbers_are_even:
    print('All numbers are even.')
