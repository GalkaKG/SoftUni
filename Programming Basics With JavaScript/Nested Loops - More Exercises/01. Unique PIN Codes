first_num_to = int(input())
second_num_to = int(input())
third_num_to = int(input())

for i in range(1, first_num_to + 1):
    for j in range(1, second_num_to + 1):
        for k in range(1, third_num_to + 1):
            if i % 2 == 0 and k % 2 == 0:
                if j == 2 or j == 3 or j == 5 or j == 7:
                    print(f'{i} {j} {k}')
