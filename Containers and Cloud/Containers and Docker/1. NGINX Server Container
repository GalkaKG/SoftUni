# Run this commands in the Terminal


docker pull nginxdemos/hello
docker images
docker run -d nginxdemos/hello 
docker ps
docker rm f9d -f
docker run -p 5000:80 -d nginxdemos/hello
docker run -p 5000:80 -d --name code_it_up nginxdemos/hello
docker logs f09
docker stop f09
docker rm f09
docker rmi nginxdemos/hello  # remove the image
